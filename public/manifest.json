{
  "manifest_version": 3,
  "name": "Proxy Switcher",
  "description": "A proxy switcher.",
  "version": "0.12.0",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "pages/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "pages/options.html",
  "permissions": ["storage", "proxy"]
}
